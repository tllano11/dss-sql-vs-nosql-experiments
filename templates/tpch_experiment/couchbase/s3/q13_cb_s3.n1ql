select
	c_count,
	count(*) as custdist
from	(
		select
			c.id as c_custkey,
			count(o.id) as c_count
		from
			c left outer join ol o on c.id = o.o_custkey
			and o.o_comment not like '%express%packages%'
		group by
			c.id
			
	) as c_orders
group by
	c_count
order by
	custdist desc,
	c_count desc;
